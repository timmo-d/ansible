tunnel: {{ tunnel_id_result.stdout }}
credentials-file: /etc/cloudflared/{{ tunnel_id_result.stdout }}.json
ingress:
{% for rule in cloudflared_ingress %}
  - {% if rule.hostname is defined %}hostname: {{ rule.hostname }}
    service: {{ rule.service }}
    {% else %}service: {{ rule.service }}
    {% endif %}
{% endfor %}
